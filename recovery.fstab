# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]       [fsoptions2]

/boot       bml     boot
/recovery   bml     recovery
/cache      rfs   /dev/block/stl11        NULL        rfs         NULL              rw,nosuid,nodev,check=no
/data       rfs    /dev/block/stl10        NULL        rfs         NULL              rw,nosuid,nodev,check=no
/sdcard     vfat    /dev/block/mmcblk0p1
/sd-ext     ext3    /dev/block/mmcblk0p2
/system     rfs    /dev/block/stl9         NULL        rfs         NULL              check=no
